entrypoint = "main.py"
modules = ["python-3.11"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

# The 'run' config below is used by the "Run" button in Replit.
[deployment]
run = "uvicorn main:app --host=0.0.0.0 --port=8000"

deploymentTarget = "cloudrun"

[[ports]]
localPort = 8000
externalPort = 80
